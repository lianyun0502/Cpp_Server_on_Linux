{
  "cmake.options.statusBarVisibility": "visible",
  "cmake.options.advanced": {
    "build": {
      "statusBarVisibility": "inherit",
      "inheritDefault": "visible"
    },
    "launch": {
      "statusBarVisibility": "inherit",
      "inheritDefault": "visible"
    },
    "debug": {
      "statusBarVisibility": "inherit",
      "inheritDefault": "visible"
    }
  },
  "makefile.makefilePath": "${workspaceFolder)",
  "C_Cpp.intelliSenseEngineFallback": "enabled",
  "liveshare.accessibility.accessibilityFeaturesEnabled": "on",
  "liveshare.allowGuestDebugControl": true,
  "liveshare.allowGuestTaskControl": true,
  "liveshare.alwaysFollowHost": true,
  "files.associations": {
    "array": "cpp",
    "*.tcc": "cpp",
    "memory": "cpp",
    "istream": "cpp",
    "functional": "cpp",
    "tuple": "cpp",
    "utility": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "cmath": "cpp",
    "compare": "cpp",
    "concepts": "cpp",
    "string": "cpp",
    "unordered_map": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "memory_resource": "cpp",
    "random": "cpp",
    "string_view": "cpp",
    "type_traits": "cpp",
    "iosfwd": "cpp",
    "limits": "cpp",
    "numeric": "cpp",
    "streambuf": "cpp",
    "system_error": "cpp",
    "typeinfo": "cpp",
    "cstring": "cpp",
    "iostream": "cpp",
    "deque": "cpp",
    "vector": "cpp",
    "initializer_list": "cpp",
    "map": "cpp",
    "condition_variable": "cpp",
    "chrono": "cpp"
  }
}